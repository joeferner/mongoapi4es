{
  "name": "mongoapi4es",
  "version": "0.0.1",
  "description": "Mongo DB API for reading and writing to Elasticsearch",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc && echo 'DONE'",
    "build:watch": "tsc --watch",
    "start": "ts-node index.ts",
    "start:watch": "DEBUG=klm:* nodemon --watch 'src/**/*.ts' --exec ts-node index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint --fix -p tsconfig.json",
    "lint-staged": "lint-staged",
    "test": "DEBUG=klm:* jest",
    "test:watch": "DEBUG=klm:* jest --watch",
    "pre-commit": "npm run format && npm run lint-staged && (cd public && npm run format && npm run lint-staged)",
    "pre-push": "npm run test && (cd public && npm run test)"
  },
  "keywords": [
    "mongo",
    "elasticsearch",
    "es",
    "mongodb"
  ],
  "author": "Joe Ferner <joe@fernsroth.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.10",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "@elastic/elasticsearch": "^7.7.1"
  }
}
